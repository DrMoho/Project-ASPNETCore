@model EvalModel
<form asp-action="SaveProduct" asp-controller="Laptop" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Cmt">Đánh giá</label>
        <input asp-for="Cmt" class="form-control" required>
    </div>
    <div class="form-group">
        <label asp-for="Rate">Giá:</label>
        <input asp-for="Rate" type="number" class="form-control" required>
    </div>
    <button type="submit" class="btn btn-primary">Tạo mới Laptop</button>
</form>
