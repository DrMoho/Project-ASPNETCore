@model List<Laptop>

@{
    ViewData["Title"] = "Home Page";
}

<section class="container py-5">
    <div>
        <a asp-controller="Laptop" asp-action="Create">
            Create new
        </a>
    </div>
    <div class="row gy-4">
        @foreach (var laptop in Model)
        {
            <partial name="LaptopSummary" model="laptop" />
        }
    </div>
</section>

